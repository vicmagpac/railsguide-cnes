<%= form_with model: estabelecimento do |form| %>
    <div>
        <%= form.label :nome %><br>
        <%= form.text_field :nome %><br>
        <% estabelecimento.errors.full_messages_for(:nome).each do |message| %>
            <div><%= message %></div>
        <% end %>
    </div>

    <div>
        <%= form.label :cnes %><br>
        <%= form.text_field :cnes %><br>
        <%= estabelecimento.errors.full_messages_for(:cnes).each do |message| %>
            <div><%= message %></div>
        <% end %>
    </div>

    <div>
        <%= form.submit %>
    </div>
<% end %>